{
  "productName": "Yao",
  "version": "0.1.0",
  "identifier": "com.yao.chat",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "vite",
    "devUrl": "http://localhost:5176",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Yao",
        "label": "main",
        "width": 1100,
        "height": 720,
        "resizable": true,
        "decorations": true
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["main-capability"]
    }
  },
  "bundle": {
    "active": false,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ]
  }
}


